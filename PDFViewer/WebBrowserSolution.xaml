<UserControl x:Class="PDFViewer.WebBrowserSolution"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:PDFViewer"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="200" />
        </Grid.ColumnDefinitions>

        <WebBrowser x:Name="webBrowser" />

        <StackPanel Grid.Column="1" Background="AliceBlue">

            <Label Content="Source" />
            <TextBox x:Name="sourceTextBox" />
            <Button Content="Load" Click="Load_Click" />
            <Button Content="Load Szakdolgozat" Click="Szakdolgozat_click" />
            <Button Content="Load Lagrange" Click="Lagrange_click" />
        </StackPanel>
    </Grid>
</UserControl>